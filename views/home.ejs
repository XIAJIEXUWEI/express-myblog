<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="particles.js is a lightweight JavaScript library for creating particles.">
    <meta name="author" content="Vincent Garreau" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title></title>
    <link rel="stylesheet" media="screen" href="/css/bootstrap.min.css">
    <link rel="stylesheet" media="screen" href="/css/main.css">
</head>

<body>
    <!-- 头部导航 -->
    <%- include('header') %>
    <!-- 内容 -->
    <div class="container">
        <div class="left-box col-lg-3 col-md-3 col-sm-4 col-xs-12 left">
            <div class="left-content">
                <div class="left-header">
                    <div class="back"></div>
                    <div class="img-box">
                        <img src="/images/samuel-zeller-336980-unsplash.jpg" alt="夏先生头像">
                    </div>
                </div>
                <h3>夏先生</h3>
                <p class="info">Graphic Designer at Self Employed</p>
                <div class="original-article">
                    <p>原创文章</p>
                    <p>66篇</p>
                </div>
                <div class="reprint-article">
                    <p>转载文章</p>
                    <p>88篇</p>
                </div>
            </div>
        </div>
        <div class="center-box col-lg-6 col-md-6 col-sm-8 col-xs-12">
            <div class="container-fluid">
                <%- include('swiper') %>
                
                <% datas.forEach((item, index) => { %>
                <div class="media content">
                    <div class="media-left media-middle">
                        <a href="/article/code/<%= item.code %>">
                            <img class="media-object" src="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1567489592943&di=d044e546c578c2185677c5d6ee19a2d4&imgtype=0&src=http%3A%2F%2Fmarket.designmodo.com%2Fwp-content%2Fuploads%2F2015%2F06%2Fbootstrap-logo.png" alt="">
                        </a>
                    </div>
                    <div class="media-body content-txt media-middle">
                        <a href="/article/code/<%= item.code %>">
                            <h4 class="media-heading">
                                <p class="content-title f-left"><%= item.title %></p>
                                <p class="content-label f-right">
                                    <span class="label label-success">bootstarp</span>
                                </p>
                                <div class="clear"></div>
                            </h4>
                            <p class="content-txt-info"><%= item.info %></p>
                            <p class="author"><span>牛舍</span>&nbsp|&nbsp<span>2019/9/16</span></p>
                        </a>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
        <div class="right-box col-lg-3 col-md-3 col-sm-12 col-xs-12 right">
            <div class="right-top">
                <div class="about-main">
                    <img src="/images/web.jpg" alt="logo">
                </div>
                <h3>Track Time on Workwise</h3>
                <p>Pay only for the Hours worked</p>
                <span>about us</span>
            </div>
        </div>
        <div class="clear"></div>
    </div>
    <script src="https://code.jquery.com/jquery-3.4.1.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script type="text/javascript">
    </script>
</body>

</html>